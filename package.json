{
  "name": "neemsah_backend",
  "version": "0.0.0",
  "scripts": {
    "build": "npx ts-node build.ts",
    "lint": "npx eslint --ext .ts src/",
    "lint:tests": "npx eslint --ext .ts spec/",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@services": "dist/services",
    "@repos": "dist/repos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/server",
    "@routes": "dist/routes",
    "@util": "dist/util",
    "@controllers": "dist/controllers",
    "@views": "dist/views"
  },
  "engines": {
    "node": ">=10.24.1"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/multer": "^1.4.7",
    "@types/node-cron": "^3.0.7",
    "@types/nodemailer": "^6.4.7",
    "@types/validatorjs": "^3.15.0",
    "aws-sdk": "^2.1342.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "cluster": "^0.7.7",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-easy-zip": "^1.1.5",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "jet-logger": "^1.3.1",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "mongoose": "^7.6.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "openai": "^4.52.7",
    "os": "^0.1.2",
    "puppeteer": "^22.12.1",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jasmine": "^4.3.1",
    "@types/jsonfile": "^6.1.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^18.15.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.1.1",
    "jasmine": "^4.6.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2"
  }
}
